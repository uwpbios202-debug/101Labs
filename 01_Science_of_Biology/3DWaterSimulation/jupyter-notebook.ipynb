   "source": [
    "# Simulate hydrogen bonding between water molecules\n",
    "def create_water_molecule(position, rotation=0):\n",
    "    \"\"\"Create a water molecule at given position with rotation\"\"\"\n",
    "    # Rotation matrix for given angle\n",
    "    theta = np.radians(rotation)\n",
    "    rot_matrix = np.array([\n",
    "        [np.cos(theta), -np.sin(theta), 0],\n",
    "        [np.sin(theta), np.cos(theta), 0],\n",
    "        [0, 0, 1]\n",
    "    ])\n",
    "    \n",
    "    # Create molecule coordinates\n",
    "    mol_oxygen = position\n",
    "    mol_hydrogen1 = position + rot_matrix @ hydrogen1\n",
    "    mol_hydrogen2 = position + rot_matrix @ hydrogen2\n",
    "    \n",
    "    return {\n",
    "        'oxygen': mol_oxygen,\n",
    "        'hydrogen1': mol_hydrogen1,\n",
    "        'hydrogen2': mol_hydrogen2,\n",
    "        'position': position\n",
    "    }\n",
    "\n",
    "# Create multiple water molecules\n",
    "molecules = [\n",
    "    create_water_molecule(np.array([0, 0, 0]), 0),\n",
    "    create_water_molecule(np.array([3, 0, 0]), 30),\n",
    "    create_water_molecule(np.array([0, 3, 0]), 60),\n",
    "    create_water_molecule(np.array([-3, 0, 0]), 90)\n",
    "]\n",
    "\n",
    "# Visualize hydrogen bonding network\n",
    "fig = plt.figure(figsize=(12, 10))\n",
    "ax = fig.add_subplot(111, projection='3d')\n",
    "\n",
    "colors = ['red', 'blue', 'green', 'purple']\n",
    "\n",
    "for i, molecule in enumerate(molecules):\n",
    "    color = colors[i % len(colors)]\n",
    "    \n",
    "    # Plot atoms\n",
    "    ax.scatter(*molecule['oxygen'], color='red', s=150, alpha=0.7)\n",
    "    ax.scatter(*molecule['hydrogen1'], color='white', s=80, alpha=0.7, edgecolors='black')\n",
    "    ax.scatter(*molecule['hydrogen2'], color='white', s=80, alpha=0.7, edgecolors='black')\n",
    "    \n",
    "    # Plot bonds\n",
    "    ax.plot([molecule['oxygen'][0], molecule['hydrogen1'][0]], \n",
    "            [molecule['oxygen'][1], molecule['hydrogen1'][1]], \n",
    "            [molecule['oxygen'][2], molecule['hydrogen1'][2]], \n",
    "            color='gray', alpha=0.7)\n",
    "    ax.plot([molecule['oxygen'][0], molecule['hydrogen2'][0]], \n",
    "            [molecule['oxygen'][1], molecule['hydrogen2'][1]], \n",
    "            [molecule['oxygen'][2], molecule['hydrogen2'][2]], \n",
    "            color='gray', alpha=0.7)\n",
    "\n",
    "# Add hydrogen bonds (dashed lines)\n",
    "# These would be between oxygen of one molecule and hydrogen of another\n",
    "for i in range(len(molecules)):\n",
    "    for j in range(i+1, len(molecules)):\n",
    "        # Simple approximation: draw line between oxygens\n",
    "        oxy1 = molecules[i]['oxygen']\n",
    "        oxy2 = molecules[j]['oxygen']\n",
    "        distance = np.linalg.norm(oxy1 - oxy2)\n",
    "        \n",
    "        # Draw hydrogen bond if molecules are close enough\n",
    "        if distance < 3.5:  # Typical hydrogen bond distance\n",
    "            ax.plot([oxy1[0], oxy2[0]], \n",
    "                    [oxy1[1], oxy2[1]], \n",
    "                    [oxy1[2], oxy2[2]], \n",
    "                    'b--', alpha=0.5, linewidth=1, label='Hydrogen bond')\n",
    "\n",
    "ax.set_xlabel('X (Å)')\n",
    "ax.set_ylabel('Y (Å)')\n",
    "ax.set_zlabel('Z (Å)')\n",
    "ax.set_title('Hydrogen Bonding Network in Water', fontsize=14, fontweight='bold')\n",
    "ax.grid(True)\n",
    "ax.set_box_aspect([1,1,1])\n",
    "\n",
    # Add legend\n",
    "from matplotlib.patches import Patch\n",
    "legend_elements = [\n",
    "    Patch(facecolor='red', alpha=0.7, label='Oxygen atoms'),\n",
    "    Patch(facecolor='white', edgecolor='black', label='Hydrogen atoms'),\n",
    "    Patch(facecolor='none', edgecolor='blue', linestyle='--', label='Hydrogen bonds')\n",
    "]\n",
    "ax.legend(handles=legend_elements)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
