    "# Plot atoms\n",
    "ax.scatter(*oxygen, color='red', s=200, label='Oxygen (δ⁻)', edgecolors='black')\n",
    "ax.scatter(*hydrogen1, color='white', s=100, label='Hydrogen (δ⁺)', edgecolors='black')\n",
    "ax.scatter(*hydrogen2, color='white', s=100, edgecolors='black')\n",
    "\n",
    "# Plot bonds\n",
    "ax.plot([oxygen[0], hydrogen1[0]], [oxygen[1], hydrogen1[1]], [oxygen[2], hydrogen1[2]], \n",
    "        'gray', linewidth=3, label='Covalent bond')\n",
    "ax.plot([oxygen[0], hydrogen2[0]], [oxygen[1], hydrogen2[1]], [oxygen[2], hydrogen2[2]], \n",
