    "# Calculate dipole moment\n",
    "# Partial charges: O = -0.834 e, H = +0.417 e each\n",
    "# 1 e = 4.803×10⁻¹⁰ esu = 1.602×10⁻¹⁹ C\n",
    "\n",
    "def calculate_dipole_moment(oxygen_pos, hydrogen1_pos, hydrogen2_pos):\n",
    "    \"\"\"Calculate the dipole moment of water molecule\"\"\"\n",
    "    # Partial charges in elementary charge units (e)\n",
    "    q_o = -0.834  # Oxygen partial charge\n",
    "    q_h = 0.417   # Hydrogen partial charge\n",
    "    \n",
    "    # Convert positions to meters (1 Å = 1e-10 m)\n",
    "    oxygen_pos_m = oxygen_pos * 1e-10\n",
    "    hydrogen1_pos_m = hydrogen1_pos * 1e-10\n",
    "    hydrogen2_pos_m = hydrogen2_pos * 1e-10\n",
    "    \n",
    "    # Elementary charge in Coulombs\n",
    "    e = 1.602176634e-19\n",
    "    \n",
    "    # Calculate dipole moment vector\n",
    "    # μ = Σ q_i * r_i\n",
    "    mu = (q_o * e * oxygen_pos_m + \n",
    "          q_h * e * hydrogen1_pos_m + \n",
    "          q_h * e * hydrogen2_pos_m)\n",
    "    \n",
    "    # Magnitude in Debye (1 D = 3.33564e-30 C·m)\n",
    "    mu_magnitude = np.linalg.norm(mu)\n",
    "    mu_debye = mu_magnitude / 3.33564e-30\n",
    "    \n",
    "    return mu, mu_magnitude, mu_debye\n",
    "\n",
    "# Calculate dipole moment\n",
    "mu, mu_mag, mu_D = calculate_dipole_moment(oxygen, hydrogen1, hydrogen2)\n",
    "\n",
    "print(\"Dipole moment calculation:\")\n",
    "print(f\"Dipole moment vector: [{mu[0]:.2e}, {mu[1]:.2e}, {mu[2]:.2e}] C·m\")\n",
    "print(f\"Magnitude: {mu_mag:.2e} C·m\")\n",
    "print(f\"Magnitude: {mu_D:.2f} D (Debye)\")\n",
    "print(f\"\\nExperimental value: ~1.85 D\")\n",
    "print(f\"Difference: {abs(mu_D - 1.85):.2f} D\")"
   ]
