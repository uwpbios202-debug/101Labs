    "# Calculate physical properties\n",
    "def calculate_water_properties():\n",
    "    \"\"\"Calculate various properties of water\"\"\"\n",
    "    properties = {}\n",
    "    \n",
    "    # Molar mass (g/mol)\n",
    "    properties['molar_mass'] = 18.015\n",
    "    \n",
    "    # Density at different temperatures (g/cm³)\n",
    "    properties['density'] = {\n",
    "        0: 0.99987,    # At 0°C\n",
    "        4: 1.00000,    # At 4°C (maximum density)\n",
    "        20: 0.99823,   # At 20°C\n",
    "        100: 0.95865   # At 100°C\n",
    "    }\n",
    "    \n",
    "    # Boiling and freezing points (°C at 1 atm)\n",
    "    properties['boiling_point'] = 100.0\n",
    "    properties['freezing_point'] = 0.0\n",
    "    \n",
    "    # Heat capacity (J/g·K)\n",
    "    properties['specific_heat'] = 4.184\n",
    "    \n",
    "    # Surface tension (mN/m at 20°C)\n",
    "    properties['surface_tension'] = 72.8\n",
    "    \n",
    "    # Dielectric constant\n",
    "    properties['dielectric_constant'] = 78.4\n",
    "    \n",
    "    # Viscosity (mPa·s at 20°C)\n",
    "    properties['viscosity'] = 1.002\n",
    "    \n",
    "    return properties\n",
    "\n",
    "# Calculate and display properties\n",
    "water_props = calculate_water_properties()\n",
    "\n",
    "print(\"Water Physical Properties:\\n\")\n",
    "print(f\"Molar Mass: {water_props['molar_mass']} g/mol\")\n",
    "print(f\"\\nDensity:\")\n",
    "for temp, density in water_props['density'].items():\n",
    "    print(f\"  At {temp}°C: {density:.5f} g/cm³\")\n",
    "print(f\"\\nPhase Change Temperatures (at 1 atm):\")\n",
    "print(f\"  Freezing point: {water_props['freezing_point']}°C\")\n",
    "print(f\"  Boiling point: {water_props['boiling_point']}°C\")\n",
    "print(f\"\\nOther Properties:\")\n",
    "print(f\"  Specific heat capacity: {water_props['specific_heat']} J/g·K\")\n",
